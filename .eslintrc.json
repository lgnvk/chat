{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint-config-airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      { "allow": ["foo_", "_bar"] },
      { "allowAfterThis": true }
    ]
  }
}
